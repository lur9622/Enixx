/* */
.red_mask {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0px;
  margin-left:-15px;
}
.red_mask_parent {
  position: absolute;
  width: 100%;
  height: 100%;
  display:none;
  top: 0px;
  background:rgba(1,1,1,0);
  transition:background 500ms;
}
.s_g_element>div>img:hover+div{
  display:block;
}
.s_g_element>div {
  position:relative;
}
.s_g_element>div>img {
  width:100%;
}
#s_gallery {
  padding:3vw 0px;
}
.red_mask_parent:hover {
  display:block;
  background:rgba(205,64,71,0.87);
  cursor:pointer;
  transition:background 500ms;
}
.r_m_center {
  position:absolute;
  top:50%;
  left:50%;
  width:68px;
  height:68px;
  margin-left:-34px;
  margin-top:-34px;
}
